<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/main.css">
    <title>Teams</title>
</head>
<body>
    <div class="content">
        <ul class="breadcrumbs">
            <li> <a href="/"> Dash</a> </li>
            <li> / </li>
            <li> Teams </li>
        </ul>
        <h1> Teams </h1>

        <section class="dataTable">
            <table class="data">
                <tr>
                    <th>Team ID</th>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
                <% teams.forEach(team => { %>
                <tr>
                    <td><%- team.Team_ID %></td>
                    <td><%- team.Team_Name %></td>
                    <td><%- team.Team_Email %></td
                </tr>
                <% }) %>
            </table>
        </section>
    </div>
</body>
</html>
