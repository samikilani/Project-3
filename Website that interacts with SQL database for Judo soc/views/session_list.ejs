<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/main.css">
    <title>Sessions</title>
</head>
<body>
    <div class="content">
        <ul class="breadcrumbs">
            <li> <a href="/"> Dash</a> </li>
            <li> / </li>
            <li> Sessions </li>
        </ul>
        <h1> Sessions </h1>

        <section class="dataTable">
            <table class="data">
                <tr>
                    <th>Session ID</th>
                    <th>Judo Moves</th>
                    <th>Date</th>
        
                </tr>
                <% sessions.forEach(session => { %>
                <tr>
                    <td><%- session.Session_ID %></td>
                    <td><%- session.Judo_Moves %></td>
                    <td><%- session.Date %></td>
               
                </tr>
                <% }) %>
            </table>
        </section>
    </div>
</body>
</html>
